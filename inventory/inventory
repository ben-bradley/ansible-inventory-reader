[a]
a.example.com

[b:children]
b.example.com

[c:children]
c.example.com

[c:vars]
foo=bar

[letter_range]
[d:f].example.com

[number_range]
number-[1:3].example.com

[combo_range]
letter-[a:b]-number-[1:2].example.com

[host_vars]
d.example.com bar=baz

[all_example]
a
b
c
